  "scripts": {
    "build": "node node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start:test": "cross-env NODE_ENV=test nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "test:ci": "jest --coverage --passWithNoTests --silent",
    "init-data": "ts-node src/init-data.ts",
    "test:auth": "jest test/unit/auth test/integration/auth",
    "test:plan": "jest test/unit/plan test/integration/plan",
    "test:demo": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:all": "npm run test && npm run test:e2e",
    "db:init": "ts-node scripts/init-database.ts",
    "seed:dev": "ts-node scripts/seed-data.ts",
    "seed:test": "ts-node scripts/seed-test-db.ts",

    "db:reset": "npm run db:init && npm run db:seed"
  },
  "dependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
